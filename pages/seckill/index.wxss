page {
  padding-bottom: 90rpx;
  background-color: #f0f0f0;
}
.revice_address_row {
  background-color: #fff;
  border-radius: 20rpx;
  margin: 20rpx;
  display: flex;
  align-items: center;
}
.revice_address_row .showtext {
  flex: 12;
  padding: 20rpx;
  font-size: 24rpx;
  color: #666;
}
.revice_address_row .user_info_row {
  flex: 12;
  padding: 20rpx;
  font-size: 24rpx;
  color: #666;
}
.revice_address_row .user_info_row .user_info {
  display: flex;
  padding: 10rpx 20rpx;
}
.revice_address_row .user_info_row .user_phone {
  margin-left: 30rpx;
}
.revice_address_row .address {
  font-weight: 400;
  font-size: 28rpx;
  color: #000;
  padding: 10rpx 20rpx;
}
.revice_address_row .right {
  flex: 1;
  color: #ccc;
  font-size: 50rpx;
  padding: 20rpx;
}
/*mask*/
.drawer_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
}
/*content*/
.drawer_attr_box {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 1001;
  background: #fff;
}
.drawer_content {
  padding: 20rpx;
  height: 800rpx;
  overflow-y: scroll;
}
.drawer_title {
  padding: 20rpx;
  font: 42rpx "microsoft yahei";
  text-align: center;
  border-bottom: 1px solid #eee;
}
.sumbit {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  background-color: var(--themeColor);
  color: #fff;
  font-weight: bold;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.drawer_coupon {
  background-color: #ffd9dd;
  color: var(--themeColor);
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
  margin: 20rpx;
  padding: 20rpx;
}
.drawer_coupon .coupon_num {
  font-size: 40rpx;
  font-weight: 400;
  margin: 0 40rpx;
}
.drawer_coupon .coupon_center {
  margin: 0 40rpx;
}
.drawer_coupon .coupon_center .start_time {
  font-size: 20rpx;
}
.drawer_coupon .coupon_center .end_time {
  font-size: 20rpx;
}
.drawer_coupon .lingqu {
  background-color: var(--themeColor);
  color: #fff;
  padding: 10rpx;
  border-radius: 15rpx;
}
.drawer_coupon .yilingqu {
  background-color: #deaaa5;
  color: #fff;
  padding: 10rpx;
  border-radius: 15rpx;
}
.cart_content {
  background-color: #fff;
  border-radius: 20rpx;
  margin: 20rpx;
}
.cart_content .cart_title {
  padding: 20rpx;
  font-size: 30rpx;
  margin-left: 20rpx;
  border-bottom: 1rpx solid #eee;
}
.cart_content .cart_main .cart_item {
  display: flex;
  padding: 20rpx;
}
.cart_content .cart_main .cart_item .cart_chk_warp {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cart_content .cart_main .cart_item .cart_img_warp {
  flex: 2;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cart_content .cart_main .cart_item .cart_img_warp image {
  width: 80%;
}
.cart_content .cart_main .cart_item .cart_info_warp {
  flex: 4;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.cart_content .cart_main .cart_item .cart_info_warp .goods_name {
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  color: #666;
  font-size: 28rpx;
}
.cart_content .cart_main .cart_item .cart_info_warp .goods_value {
  display: flex;
  justify-content: space-between;
  font-size: 22rpx;
  color: #b4b4b4;
  margin: 20rpx;
  margin-left: 0;
}
.cart_content .cart_main .cart_item .cart_info_warp .goods_norm {
  font-size: 24rpx;
}
.cart_content .cart_main .cart_item .cart_info_warp .goods_price_warp {
  display: flex;
  justify-content: space-between;
}
.cart_content .cart_main .cart_item .cart_info_warp .goods_price_warp .price {
  display: flex;
  align-items: center;
}
.cart_content .cart_main .cart_item .cart_info_warp .goods_price_warp .goods_price {
  color: var(--themeColor);
  font-size: 30rpx;
}
.cart_content .cart_main .cart_item .cart_info_warp .goods_price_warp .goods_oldprice {
  text-decoration: line-through;
  color: #c8c8c8;
  font-size: 24rpx;
  margin-left: 20rpx;
}
.cart_content .cart_main .cart_item .cart_info_warp .goods_price_warp .cart_num_tool {
  display: flex;
}
.cart_content .cart_main .cart_item .cart_info_warp .goods_price_warp .cart_num_tool .num_edit {
  width: 55rpx;
  height: 55rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1px solid #ccc;
}
.cart_content .cart_main .cart_item .cart_info_warp .goods_price_warp .cart_num_tool .goods_num {
  width: 55rpx;
  height: 55rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-top: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
}
.bottom {
  background-color: #fff;
  border-radius: 20rpx;
  margin: 20rpx;
  padding: 20rpx;
}
.bottom .coupon {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx;
  color: #000;
}
.bottom .coupon .coupon_num {
  font-weight: 400;
  display: flex;
  align-items: center;
}
.bottom .coupon .right_btn {
  color: #bbb;
  font-size: 40rpx;
  margin-left: 10rpx;
}
.bottom .goods_num {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx;
  color: #000;
}
.bottom .goods_num .goods_total {
  font-weight: 400;
}
.bottom .all_minus {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx;
  color: #000;
}
.bottom .all_minus .minus {
  font-weight: 400;
  color: var(--themeColor);
}
.bottom .total {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx;
  color: #000;
}
.bottom .total .total_price {
  font-weight: 400;
  color: var(--themeColor);
}
.footer_tool {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 90rpx;
  background-color: #fff;
  display: flex;
  border-top: 1rpx solid #ccc;
}
.footer_tool .total_price_warp {
  flex: 4;
  padding-left: 15rpx;
  text-align: left;
}
.footer_tool .total_price_warp .total_price .total_price_text {
  color: var(--themeColor);
  font-size: 34rpx;
  font-weight: 600;
}
.footer_tool .order_pay_warp {
  flex: 3;
  background-color: var(--themeColor);
  color: #fff;
  font-size: 32rpx;
  font-weight: 600;
  display: flex;
  justify-content: center;
  align-items: center;
}
