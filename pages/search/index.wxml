<view class="search_row">
    <input value="{{inputValue}}" placeholder="请输入您要搜索的商品" bindinput="handleInput"></input>
    <button size="mini" bindtap="handleCancel" hidden="{{!isFocus}}">取消</button>
</view>
<view class="search_content">
    <navigator url="/pages/goods_detail/index?goodsId={{item.goodsId}}" class="search_item" wx:for="{{goods}}" wx:key="goodsId">
        {{item.goodsName}}
    </navigator>
</view>